<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.three.shop.mapper.ProductDetailMapper">
    <resultMap id="BaseResultMap" type="ProductDetail">
        <id property="productDetailId" column="product_detail_id"/>
        <result property="price" column="price"/>
        <result property="img" column="img"/>
        <result property="size" column="size"/>
        <result property="createTime" column="create_time"/>
        <result property="unloadTime" column="unload_time"/>
        <result property="stock" column="stock"/>
        <result property="status" column="status"/>
    </resultMap>
</mapper>